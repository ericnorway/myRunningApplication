<Window x:Class="MyRunningApplication.GraphWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyRunningApplication"
        xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        mc:Ignorable="d"
        Title="Graph Window" Height="500" Width="660" Background="#FF1FCAD4">
    <Grid>
        <WindowsFormsHost Margin="10">
            <wf:Panel x:Name="graphPanel"/>
        </WindowsFormsHost>
    </Grid>
</Window>
